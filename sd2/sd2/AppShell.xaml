<?xml version="1.0" encoding="UTF-8" ?>
<Shell 
    x:Class="sd2.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:sd2"
    Shell.FlyoutBehavior="Disabled">

    <TabBar>
        <Tab Title="Схема визуализации 1">
            <ShellContent
                x:Name="FirstScheme"
                ContentTemplate="{DataTemplate local:FirstScheme}"
                Route="FirstScheme" />     
        </Tab>
        <Tab Title="Схема визуализации 2">
            <ShellContent
                x:Name="SecondScheme"
                ContentTemplate="{DataTemplate local:SecondScheme}"
                Route="SecondScheme" />
        </Tab>
    </TabBar>

</Shell>
